{
    "plugins": [
        "ejs",
        "js"
    ],
    "extends": [
        "airbnb",
        "airbnb/hooks"
    ],
    "rules": {
        "linebreak-style": 0,
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ]
    }
}